<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.jenkins-ci.plugins</groupId>
  <artifactId>job-dsl</artifactId>
  <version>1.87</version>
  <packaging>hpi</packaging>
  <name>Job DSL</name>
  <description>The Jenkins Plugins Parent POM Project</description>
  <url>https://github.com/jenkinsci/job-dsl-plugin</url>
  <inceptionYear>2016</inceptionYear>
  <licenses>
    <license>
      <name>Apache License, Version 2.0</name>
      <url>https://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <id>quidryan</id>
      <name>Justin Ryan</name>
      <email>jryan@netflix.com</email>
    </developer>
    <developer>
      <id>andrewharmellaw</id>
      <name>Andrew Harmel-Law</name>
      <email>andrew@harmel-law.com</email>
    </developer>
    <developer>
      <id>daspilker</id>
      <name>Daniel Spilker</name>
      <email>mail@daniel-spilker.com</email>
    </developer>
    <developer>
      <id>jamietanna</id>
      <name>Jamie Tanna</name>
      <email>jenkins@jamietanna.co.uk</email>
